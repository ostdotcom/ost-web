              <select class="form-control selectpicker" id="categorySelect">
                <option value="all"><%=  display_labels[:all] %></option>
                <option value="social-creative"><%=  display_labels[:'social-creative'] %></option>
                <option value="loyalty"><%=  display_labels[:'loyalty'] %></option>
                <option value="dapps"><%=  display_labels[:'dapps'] %></option>
                <option value="marketplace"><%=  display_labels[:'marketplace'] %></option>
                <option value="community"><%=  display_labels[:'community'] %></option>
                <option value="sharing-economy"><%=  display_labels[:'sharing-economy'] %></option>
              </select>
            </div>
          </div>
          <div class="col-6 border-class">
            <div class="form-group">
              <label for="sizeSelect" class="filter-style">SIZE</label>
              <select class="form-control selectpicker" id="sizeSelect">
                <option value="all"><%=  display_labels[:all] %></option>
                <option value="large"><%=  display_labels[:large] %></option>
                <option value="start-up"><%=  display_labels[:'start-up'] %></option>
              </select>
            </div>
          </div>
        </div>

      </div>

    </div>

    <div class="row mt-5" style="overflow: hidden">
      <% partner_companies.each_with_index do|partner, index| %>
        <div class="col-6 col-md-3 p-0 category-all size-all visible <%= 'category-'+partner[:categories].join(" category-") %> size-<%= partner[:size] %>">
          <div class="aspect-ratio-box" data-toggle="modal" data-target="#infoModal-<%=index %>" title="<%= 'category-'+partner[:categories].join(" category-") %> size-<%= partner[:size] %>">
            <span class="aspect-ratio-box-inside partner-company" style="background-position-y: <%= ((100.00/(partner_companies_count-1))* partner[:sprite_index]).round(2)%>%;" data-toggle="modal" data-target="#infoModal-<%=index %>" data-partner_companies_index="<%=index %>"></span>
            <% if partner[:has_exited] == true%>
              <div class="position-absolute" style="top: 10px;right: 10px;font-size: 0;">
                <span class="ribbon">EXITED</span>
                <span class="arrow"></span>
              </div>
            <% end %>


          </div>
        </div>


        <% content_for :end_js do %>
          <!-- Modal: infoModal-<%=index %> -->
          <div class="modal fade right modal-company" id="infoModal-<%=index %>">
            <div class="modal-dialog modal-dialog-centered">
              <div class="modal-content p-2 p-sm-5">
                <div>
                  <button type="button" class="close close-btn" data-dismiss="modal">&times;</button>
                </div>
                <div class="modal-body">
                  <div class="row">
                    <div class="col-12 col-md-9">
                      <div class="mt-3 mb-2">
                        <img src="" alt="Image not found" >
                      </div>
                      <div class="description">
                        <%= raw partner[:description_html] %>
                      </div>
                    </div>


                    <div class="col-12 col-md-3 details-style">
                      <% if partner[:founders]%>
                        <div class="founders mb-3">
                          <h3 class="text-uppercase details mt-2">Founders</h3>
                          <%  partner[:founders].each_with_index do|founder, index| %>
                            <div class="name "><%= founder[:name] %></div>
                            <div class="title mb-2"><%= founder[:title] %></div>
                          <% end %>

                        </div>
                      <% end %>
                      <% if partner[:location]%>
                        <div class="location mb-3">
                          <h3 class="text-uppercase details mt-2">Location</h3>
                          <div class="address"><%= partner[:location] %></div>
                        </div>
                      <% end %>
                      <% if partner[:categories]%>
                        <div class="industry mb-3">
                          <h3 class="text-uppercase details mt-2">Industry</h3>
                          <div class="industry-value"><%= partner[:categories].map{ |key| display_labels[key.to_sym] }.join(', ')  %></div>
                        </div>
                      <% end %>
                      <% if partner[:size]%>
                        <div class="size mb-3">
                          <h3 class="text-uppercase details mt-2" >Size</h3>
                          <div class="size-value" ><%=  display_labels[partner[:size].to_sym] %> </div>
                          <!--                          <div class="size-value"><%#= display_labels[:partner[:size]] %></div>-->
                        </div>
                      <% end %>
                      <% if partner[:website]%>
                        <div class="website mb-3">
                          <h3 class="text-uppercase details mt-2">Website</h3>
                          <div class="website-value"><a href="<%= display_labels[partner[:website].to_sym] %>" target="_blank"><%= partner[:website] %></a></div>
                        </div>
                      <% end %>
                      <% if partner[:social]%>
                        <div class="social mb-3">
                          <%  partner[:social].each_with_index do|social_info, index| %>
                            <a href="<%= display_labels[social_info[:link] .to_sym] %>"> <%= social_info[:name] %></a>
                          <% end %>
                        </div>
                      <% end %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <!-- /Modal: infoModal-<%=index %> -->
        <% end %>

      <% end %>
    </div>
  </div>

</section>