      </div>
      <div class="iframe-wrapper position-relative">
        <img class="iframe-cover-img" data-name="hornet" src="https://dxwfxs8b4lg24.cloudfront.net/ost/images/prototype-cover/hornet-v1.png" />
        <img class="position-absolute processing-loader" style="display: none" src="//dxwfxs8b4lg24.cloudfront.net/ost-kit/images/processed-loader-1.gif" width="30" height="30"/>
      </div>
      <div class="iframe-wrapper position-relative">
        <img class="iframe-cover-img" data-name="tolkin" src="https://dxwfxs8b4lg24.cloudfront.net/ost/images/prototype-cover/tolkin-v1.png" />
        <img class="position-absolute processing-loader" style="display: none" src="//dxwfxs8b4lg24.cloudfront.net/ost-kit/images/processed-loader-1.gif" width="30" height="30"/>
      </div>

    </div>
  </div>
</section>
<% content_for :end_js do %>
  <script type="text/javascript">
    (function () {
      ost.carousel.prototypes.init({
        invisionRootUrl : "<%= GlobalConstant::Base.invision_prototypes['root_url'] %>",
        invisionVersion: 2
      });
    })()
  </script>
<%end%>